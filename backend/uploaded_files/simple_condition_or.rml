logsource matches {product: 'windows', category: 'process_creation'};
no_selection1 not matches {image: 'C:\\Windows\\System32\\cmd.exe'};
no_selection2 not matches {image: 'C:\\Windows\\SysWOW64\\cmd.exe'};
no_selection3 not matches {image: 'C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe'};
Main = logsource >> (no_selection3 (Main \/ empty));